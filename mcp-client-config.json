{
  "mcpServers": {
    "supabase": {
      "command": "node",
      "args": ["mcp-server-supabase.js"],
      "env": {
        "SUPABASE_URL": "your_supabase_url_here",
        "SUPABASE_ANON_KEY": "your_supabase_anon_key_here"
      }
    },
    "TalkToFigma": {
      "command": "npx",
      "args": [
        "cursor-talk-to-figma-mcp@latest"
      ]
    }
  }
} 